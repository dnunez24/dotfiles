Host *
    AddKeysToAgent ask
    ConnectTimeout 0
    ForwardX11 no
    Include "~/.ssh/config.local"
    PreferredAuthentications publickey,password
    PubkeyAuthentication yes
    Protocol 2,1
    ServerAliveCountMax 5
    ServerAliveInterval 60
    StrictHostKeyChecking ask
    TCPKeepAlive yes
